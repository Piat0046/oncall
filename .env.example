# Docker 테스트 환경 설정
# ===========================================

# MySQL (Docker)
DOCKER_MYSQL_HOST=localhost
DOCKER_MYSQL_PORT=3307
DOCKER_MYSQL_ROOT_PASSWORD=rootpassword
DOCKER_MYSQL_USER=laplace
DOCKER_MYSQL_PASSWORD=laplace123
DOCKER_MYSQL_DATABASE=laplace

# Trino (Docker)
TRINO_HOST=localhost
TRINO_PORT=8080
TRINO_CATALOG=hive
TRINO_SCHEMA=default

# Hive Metastore (Docker)
HIVE_METASTORE_HOST=localhost
HIVE_METASTORE_PORT=9083
