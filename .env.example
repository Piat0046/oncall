# ===========================================
# DAG Validator 설정
# ===========================================

# MySQL (결과 DB - DAG 검증용)
MYSQL_HOST=localhost
MYSQL_PORT=3306
MYSQL_USER=user
MYSQL_PASSWORD=password
MYSQL_DATABASE=airflow_results
MYSQL_CHARSET=utf8mb4

# Airflow REST API (기본)
AIRFLOW_API_URL=http://localhost:8080
AIRFLOW_API_USER=admin
AIRFLOW_API_PASSWORD=admin

# PostgreSQL - Airflow DB (--use-db 옵션용)
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_USER=airflow
POSTGRES_PASSWORD=password
POSTGRES_DATABASE=airflow

# 애플리케이션 설정
APP_LOG_LEVEL=INFO
APP_OUTPUT_DIR=./output

# ===========================================
# Migration 설정
# ===========================================

# 소스 DB (마이그레이션 원본)
SOURCE_DB_HOST=localhost
SOURCE_DB_PORT=3306
SOURCE_DB_USER=user
SOURCE_DB_PASSWORD=password
SOURCE_DB_DATABASE=laplace

# 타겟 DB (마이그레이션 대상)
TARGET_DB_HOST=localhost
TARGET_DB_PORT=3307
TARGET_DB_USER=user
TARGET_DB_PASSWORD=password
TARGET_DB_DATABASE=laplace
